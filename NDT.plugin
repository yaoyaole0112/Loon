                      region = 'CN'
                  } else {
                      region = 'US'
                  }
                  console.log("YTB region:" + region);
                  result["YouTube"] = "<b>YouTube Premium: </b>æ”¯æŒ "+arrow+ "âŸ¦"+flags.get(region.toUpperCase())+"âŸ§ ğŸ‰"
                  resolve(region);
              } else {
                result["YouTube"] = "<b>YouTube Premium: </b>æ£€æµ‹è¶…æ—¶ ğŸš¦";
                resolve("timeout");
              }
            }
        })
    })
}



